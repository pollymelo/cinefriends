datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id              Int      @id @default(autoincrement())
  nome            String
  genero          Genero?
  data_nascimento DateTime
  email           String   @unique
  telefone        String
  usuario         String   @unique
  senha           String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}

enum Genero {
  Masculino
  Feminino
  Nao_Binario
} 
